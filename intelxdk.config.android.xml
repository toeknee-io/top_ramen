<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1" id="com.bitsmitten.topramen.android" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:intelxdk="http://xdk.intel.com/ns/v1">
    <intelxdk:version value="1.1" />
    <intelxdk:cordova-cli version="6.2.0" />
    <name>Top Ramen</name>
    <description>Make Ramen!</description>
    <author>BitSmitten Studios</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <intelxdk:plugin intelxdk:checksum="c0146fbc" intelxdk:name="StatusBar" intelxdk:type="file" intelxdk:value="cordova-plugin-statusbar" intelxdk:version="2.1.3" />
    <intelxdk:plugin intelxdk:checksum="2373ccea" intelxdk:name="Device" intelxdk:type="file" intelxdk:value="cordova-plugin-device" intelxdk:version="1.1.2" />
    <intelxdk:plugin intelxdk:checksum="47137083" intelxdk:name="Splashscreen" intelxdk:type="file" intelxdk:value="cordova-plugin-splashscreen" intelxdk:version="3.2.2" />
    <intelxdk:plugin intelxdk:checksum="cf9984de" intelxdk:name="Vibration" intelxdk:type="file" intelxdk:value="cordova-plugin-vibration" intelxdk:version="2.1.1" />
